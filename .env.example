# Example environment variables for EBS AI API
# Copy this to .env and fill in your actual values

# Google Cloud Storage
ENABLE_GCS=
GCS_BUCKET=

# Model paths
MODEL_PATH=models/v4.pt
KNR_MODEL_PATH=knr_models/model_knr_best.joblib
KNR_ENCODER_PATH=knr_models/encoder_target.joblib

# Google Cloud Storage credentials (if using)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/gcs-credentials.json

# Gemini AI API Key (for RAG functionality)
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase (if using for knowledge base storage)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key